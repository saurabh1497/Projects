package com.zycus.entities;

import javax.persistence.Entity;

@Entity
public class Author extends Item{

	private String name;
	private int id;
	private int age;
	
	
	public Author() {
		super();
	}


	public Author(String name, int age) {
		super();
		this.name = name;
		this.age = age;
	}


	public Author(String name, int id, int age) {
		super();
		this.name = name;
		this.id = id;
		this.age = age;
	}


	public String getName() {
		return name;
	}


	public void setName(String name) {
		this.name = name;
	}


	public int getId() {
		return id;
	}


	public void setId(int id) {
		this.id = id;
	}


	public int getAge() {
		return age;
	}


	public void setAge(int age) {
		this.age = age;
	}


	@Override
	public String toString() {
		return "/n/t Author [name=" + name + ", id=" + id + ", age=" + age + "]";
	}
	
	
	
	
	
	
	
}


